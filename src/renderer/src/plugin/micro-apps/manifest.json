{
  "version": "1.0.0",
  "description": "Example micro-apps for EcuBus-Pro plugin system",
  "apps": [
    {
      "name": "hello-world",
      "activeWhen": "/plugins/hello-world",
      "entryUrl": "hello-world/index.js",
      "customProps": {
        "title": "Hello World App",
        "theme": "light"
      }
    },
    {
      "name": "dashboard",
      "activeWhen": "/plugins/dashboard",
      "entryUrl": "dashboard/index.js",
      "customProps": {
        "refreshInterval": 5000
      }
    }
  ]
}

