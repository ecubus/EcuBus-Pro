{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2022",
    "skipLibCheck": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": ".ScriptBuild",
    "resolveJsonModule": true,
    "baseUrl": "."
  }
}
